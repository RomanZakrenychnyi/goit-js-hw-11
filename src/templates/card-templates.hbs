<div class='photo-cards'>
  <a href='{{largeImageURL}}'>
    <img src='{{webformatURL}}' alt='{{tags}}' />
  </a>
  <div class='info'>
    <p class='info-item'>
      <b>Likes</b>
      {{likes}}
    </p>
    <p class='info-item'>
      <b>Views</b>
      {{views}}
    </p>
    <p class='info-item'>
      <b>Comments</b>
      {{comments}}
    </p>
    <p class='info-item'>
      <b>Downloads</b>
      {{downloads}}
    </p>
  </div>
</div>